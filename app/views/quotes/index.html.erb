<main class="container">
  <div class="header">
    <h1>Quotes</h1>
    <%= link_to "New quote",
        new_quote_path,
        class: "btn btn--primary",
        data: { turbo_frame: dom_id(Quote.new) } %>
  </div>
<%# placeholder turbo_frame for NEW action%>
  <%= turbo_frame_tag "new_quote" %>

  <%= turbo_frame_tag "quotes" do %>
    <%= render @quotes %>
  <% end %>
</main>
